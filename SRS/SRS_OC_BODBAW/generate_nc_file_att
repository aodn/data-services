[TIME]
calendar = gregorian
units    = days since 1970-01-01T00:00:00 UTC

[LATITUDE]
reference_datum = geographical coordinates, WGS84 projection

[LONGITUDE]
reference_datum = geographical coordinates, WGS84 projection

[DEPTH]
reference_datum = sea surface

[station_name]
long_name = station name
cf_role   = timeseries_id

[profile]
long_name = profile index
cf_role   = profile_id

[station_index]
long_name          = which station this profile is for
instance_dimension = station
units              = 1
coordinates        = LATITUDE LONGITUDE TIME station_index

[row_size]
long_name        = number of observations for this profile
sample_dimension = obs
units            = 1
coordinates      = LATITUDE LONGITUDE TIME
